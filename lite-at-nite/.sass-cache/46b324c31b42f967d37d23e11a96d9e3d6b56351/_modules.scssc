// http://livetools.uiparade.com/ribbon-builder.html
.ribbon-wrap {
	position: relative;
	z-index: 200;
	padding: 0;
	background: #d21c29;
}
	.ribbon {
		position: relative;
		width: 100%;
		margin: -20px 0 0 0;
		padding: 10px;
		background: $color-red;
		border-bottom: 20px solid #fff;
	}
		.ribbon-content {
		   text-align: center;
		   color: #fff;
		   @include rem(font-size, 15px); 
		}
		.ribbon-content:before, .ribbon-content:after {
			content: '';
			position: absolute;
			display: block;

			border-color: #b91c29 transparent transparent transparent;
			border-style: solid;
			bottom: -20px;
			background: #d21c29;
		}
		.ribbon-content:before {
		   left: 0;
		   border-width: 20px 0 0 20px;
		}
		.ribbon-content:after {
		   right: 0;
		   border-width: 20px 20px 0 0;
		}

.ribbon-home .ribbon {
	border-bottom: 20px solid #666;
}

.ribbon-thanks {
	background: #d21c29;
	width: 70%;
	margin: 0 auto;
	border-top: 20px solid #fff;

	.ribbon {
		width: 85%;
		margin: -20px auto 0 auto;
	}
	h2 { margin-bottom: 10px; }
}

// ----- Home page -----

// Events panel
.events-panel {

	.item {
		background: #fff;
		padding: 15px 0;
	}
}

// About panel
.about-panel {
	color: #666;
	text-align: center;
	background-color: #eee;

	.btn-group { margin-top: 25px; }
}

// About Partners panel
.about-partners-list {

	.row { 
		padding:15px 0;
		border-bottom: 1px solid #ccc; 
	}
	.row:first-child { padding-top: 30px; }
	.row:last-child { padding-bottom: 30px; border: none; }

		p { margin-bottom:15px; }

		.toggleContent {
			float: left;
			@include rem(font-size, 18px);
			color:$color-red;
			text-transform: uppercase; 

				.icon-doubledown {
					padding-left:3px;
				}
		}

		.expand {
			display: none;
		}
}


// Donations panel
.donations-panel {
	color: #666;
	text-align: center;

	.text-container {
		padding: 20px;
		background: #fff;
	}
		.text {
			padding: 30px;
			background: #fff;
			border: 8px solid #f1f1f1;;
		}
		p {
			display: inline-block;
			margin: 0 0 25px 10px;
			padding: 6px 0 6px 20px;
			border-left: 1px solid #f1f1f1;
		}

		span { color: $color-red; }
}


// Partners panel
.partners-panel {
	text-align: center;

	a {
		display: block;
		margin: 0 0 20px 0;
		color: #333;
		border: 1px solid #e6e6e6;
	}
	h4 {
		min-height: 80px;
		padding: 20px 5px;
		margin: 0;
		background: #f1f1f1;
		border-top: 1px solid #e6e6e6;
	}
	img {
		padding-left: 0;
		margin: 0 auto;
	}
}

// Partners panel
.instagram-panel {

	iframe {
		width: 100%;
		height: 85px;
		overflow: hidden;
		border: none;
	}
}

// ----- Events - Employee relay page -----
.relay {
	position: relative;

	img { width: 100%; }
	.map {
		width: 100%;
		height: 855px;
	}
}
	.accordion, .accordion-container { width: 100%; }

		.accordion-container h3, .accordion .item-toggle, .accordion .leg-title {
			font-family: $font-header-bold;
			text-transform: uppercase;
			@include rem(font-size, 18px);
		}

		.accordion-container h3, .accordion .item-toggle { 
			margin: 0; 
			padding: 10px 15px;
			border-bottom: 1px solid #fff;
		}

		.accordion-container h3 {
			color: #fff;
			background: $color-red;
		}

	.accordion {

		.item-toggle {
			display: block;
			width: 100%;
			color: #ddd;
			background: #333;
			cursor: pointer;
			outline: none;
			@include single-transition(all, 250ms);
		}
		.item-toggle:hover {
			background: #444;
		}
		.item-toggle.ui-state-active {
			color: #333;
			background: #fff;
		}

		.item {
			background: #fff;
			display: none;

			p { margin: 0 0 10px 0; }
		}
			.item-content {
				padding: 15px;
				@include rem(font-size, 12px);
			}
				.leg-title {
					color: $color-red;
					margin-bottom: 10px;

					div:nth-child(2) { text-align: right; }
					i { margin: 0 0 0 5px; @include rem(font-size, 24px); }
					.inactive { color: #ddd; }
				}
				.leg-section {
					padding-bottom: 3px;
					margin-bottom: 6px;					
					border-bottom: 1px solid #bbb;

					&:last-of-type { border: none; }
					div:first-child { 
						font-family: $font-header-bold; 
						text-transform: uppercase; 
					}
				}
	}


// Email panel
.email-panel {
	.box-title {
		border-color: #fff;
		margin: 0;
	}
		p {
			margin-bottom:10px;
		}
		.h5, p:last-child {
			text-transform: none;
		}
}

// ----- Gallery page -----
.gallery {
	position: relative;
	max-width: 1400px;
	margin-left: auto;
	margin-right: auto;

	.cycle-gallery .item, .cycle-gallery .item img { width: 100%; }

	.overlay-text {
		position: absolute;
		bottom: 0;
		width: 100%;
		padding: 15px 0;
		color: #fff;
		background: rgba(0,0,0,.5);

		// .col-sm-2, .col-sm-10 { padding-top: 15px; padding-bottom: 15px; }
		// .col-sm-2 { border-right: 1px solid #fff; }
	}

	.cycle-carousel {
		margin: 30px 0 0 0;
	}
		.cycle-carousel .item, .cycle-carousel img {
			width: auto;
			height: auto;
			padding: 2px;
			cursor: pointer;
		}
		.cycle-carousel img {
			max-width: 94px !important;
			height: auto;
		}
}

.gallery-title {
	margin: 0;
	padding: 14px 0;
	color: #fff;
	text-align: center;
	background: $color-red;
}


// ----- Events landing page -----
.events-landing {
	color: #fff;
	border-bottom: 6px solid $color-red;

	h2 a, h3 a { color: #fff; }
	.box-title { margin: 0; }
	.triangle-up:before { top: auto; bottom: -6px; }
}
.events .events-landing:last-of-type { border: none; }
.events-landing-text { padding: 15px 0 20px; background-color: rgba(0,0,0,.4); }


// ----- Standard Text page -----
.standard-text {
	ul {
		margin-bottom:30px;
	}
	li {
		margin:0 0 0 40px;
		padding:0 0 0 20px;
		list-style-type:disc;
	}
}


/* 479px and down */
@media (max-width: $screen-xs-custom-max) {  
	.donations-panel .text {
		@include rem(font-size, 15px); 
	}
}

/* 767px and down */
@media (max-width: $screen-sm-custom-max) { 
	.ribbon-home { 
		border: none; 

		.ribbon {
			border-color: #fff; 
		}
	}

	.ribbon-thanks { width: 100%; }

	.cycle-carousel { display: none; } 
	
	.events-panel, .donations-panel, .donations-panel .text-container {
		padding: 0;
	}

	.events-panel {
		background: none !important;

	}
	.events-panel .col-sm-6 {
		border-bottom: 1px solid #ccc;
	}

	.about-partners-list {
		background:#fff;
		clear:both;
		text-align: center;

		img {
			margin: 0 auto 15px auto;
		}
		.toggleContent {
			float:none;
		}
	}

	.donations-panel {
		.text {
			padding: 15px;
		}
			img, p {
				float: left;
				width: 50%;
			}
			img {
				padding-right: 20px;
			}
			p {
				margin-right: 0;
				margin-left: 0;
			}
	}

	.events-landing h2 { text-align: center; }
	.events-landing .h2 { @include rem(font-size, 28px); }

	.news-signup-panel .form-control {
		margin-bottom: 15px;
	}

	.relay .map, .relay h3:last-child { display: none; } 
}


/* 768px and up */
@media (min-width: $screen-sm-min) { 
	.ribbon { padding: 20px; }

	.events-panel, .events-landing-text {
		padding-top: 60px;
		padding-bottom: 60px;
	}

	.events-panel .col-sm-12 {
		@include rem(font-size, 18px); 

		h3 { @include rem(font-size, 60px); }
	}
	.events-panel, .events-landing {
		background-color: #666;
	}
	.events-panel .item {
		padding: 15px 20px;
	}


	.donations-panel-home {
		margin: -130px 0 -40px 0;

		.text-container {
			padding-bottom:  0;
		}
	}

	.donations-panel-events {
		margin-top: -130px; 
	}

	.about-panel {
		padding: 40px 0 130px 0;
	}

	.news-signup-panel .form-control {
		width: 280px;
		margin-right: 15px;
	}

	.events-landing:last-of-type .events-landing-text { padding-bottom: 130px; }

	.relay .accordion-container {
		position: absolute;
		top: 15px;
		left: 30px;
		z-index: 100;
	}
	.relay .accordion {
		// height: 435px;
		height: 745px;
		overflow: scroll;
	}

	.gallery .overlay-text {
		padding: 0;

		.col-sm-2, .col-sm-10 { padding-top: 15px; padding-bottom: 15px; }
		.col-sm-2 { border-right: 1px solid #fff; }
	}
}

/* 768px - 991px */
@media (min-width: $screen-sm-custom-min) and (max-width: $screen-md-custom-max) { 
	.donations-panel p {
		margin-left: 0;
		padding: 0;
		border: none; 
	}
}
/* 768px - 1999px */
@media (min-width: $screen-sm-custom-min) and (max-width: $screen-lg-custom-max) { 
	.ribbon-home { margin-top: -40px; } 
}

/* 992px and up */
@media (min-width: $screen-md-custom-min) {  
	.gallery .cycle-carousel {
		margin: 20px 0 0 0;
	}

	// .relay .accordion {
	// 	height: 595px;
	// }
}

/* 992px - 1999px */
@media (min-width: $screen-md-custom-min) and (max-width: $screen-lg-custom-max) {  }

/* 1200px and up */
@media (min-width: $screen-lg-custom-min) { 
	.ribbon-home { margin-top: -25px; } 

	.gallery .cycle-carousel {
		margin: 10px 0 0 0;
	}

	// .relay .accordion {
	// 	height: 745px;
	// }
}
